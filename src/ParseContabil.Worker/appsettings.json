{
  "Serilog": {
    "MinimumLevel": "Warning",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7,
          "fileSizeLimitBytes": 10485760,
          "rollOnFileSizeLimit": true
        }
      }
    ]
  },
  "ConnectionStrings": {
    "ParseContabilConnection": "Server=(localdb)\\mssqllocaldb;Database=ParseContabil;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Configurations": {
    "PathInput": "input",
    "PathOutput": "output",
    "TimeVerifyDirectory": 10
  }
}
